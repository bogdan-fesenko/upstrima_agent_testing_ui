# API URL
NEXT_PUBLIC_API_URL=http://localhost:8000  # [REQUIRED] - Backend API URL, must match PORT in backend .env

# Supabase Configuration
# Replace these with your actual Supabase project values
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co  # [REQUIRED] - Must match SUPABASE_URL in backend .env
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-supabase-anon-key  # [REQUIRED] - Must match SUPABASE_KEY in backend .env